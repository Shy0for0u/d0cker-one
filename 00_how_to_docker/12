docker run -d --restart on-failure:10 --name spawning-pool -v hatchery:/var/lib/mysql -e MYSQL_ROOT_PASSWORD=Kerrigan -e MYSQL_DATABASE=zerglings  mysql --default-authentication-plugin=mysql_native_password

